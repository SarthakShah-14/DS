#include<stdio.h>

struct node
{
	int rno;
	char name[10];
	int sem;
	int sub1;
	int sub2;
	int sub3;
	int total;

	struct node *next;
}*first=NULL,*last=NULL,*curr=NULL,*tmp=NULL;

int isempty()
{
	return first==NULL;
}

void insertfirst()
{
    tmp=(struct node*)malloc(sizeof(struct node));
	printf("enter data:");
    scanf("%d",&tmp->rno);
    scanf("%s",&tmp->name);
    scanf("%d",&tmp->sem);
    scanf("%d",&tmp->sub1);
    scanf("%d",&tmp->sub2);
    scanf("%d",&tmp->sub3);
    tmp->total=tmp->sub1+tmp->sub2+tmp->sub3;
	tmp->next=first;
	if(isempty())
	{
		first =last=tmp;
	}
	else
	{
		first=tmp;
	}

}

int main()
{
	return 0;
}
