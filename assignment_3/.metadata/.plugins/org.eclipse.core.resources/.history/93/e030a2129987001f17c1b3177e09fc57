#include<stdio.h>
#include<stdlib.h>
struct node
{
	int data;
	struct node *left;
	struct node *right;
}*first=NULL,*last=NULL,*nn=NULL,*temp,*curr,*prev;
void create();
int main()
{
	create();
	return 0;
}
void create()
{
	int value;
	printf("Enter the value and -1 for end\n");
	scanf("%d",&value);
	while(value!=-1)
	{
		nn=(struct node *) malloc (sizeof(struct node));
		nn->data=value;
		nn->left=NULL;
		nn->right=NULL;

		if(first==NULL)
		{
			first=nn;
			last=nn;
		}
		else
		{
			last->right=nn;
			nn->left=last;
			last=nn;
		}
		printf("Enter the value and -1 for end\n");
		scanf("%d",&value);
	}
}
